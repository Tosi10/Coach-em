{
  "expo": {
    "name": "CoachemApp",
    "slug": "coachemapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Coach'em (1).png",
    "scheme": "coachemapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Coach'em (1).png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#0a0a0a"
      }
    },
    "android": {
      "package": "com.coachem.app",
      "splash": {
        "backgroundColor": "#0a0a0a"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Coach'em (1).png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "navigationBar": {
        "backgroundColor": "#0a0a0a",
        "barStyle": "light-content"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Coach'em (1).png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "O app precisa acessar suas fotos para escolher uma imagem ou vídeo.",
          "cameraPermission": "O app precisa da câmera para gravar vídeos de exercícios."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0a0a0a",
          "image": "./assets/images/Coach'em (1).png",
          "imageWidth": 280,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "#0a0a0a",
          "barStyle": "light-content"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "882f63be-2b20-4a26-8b98-a9c1538e0b2d"
      }
    },
    "owner": "v6core"
  }
}
